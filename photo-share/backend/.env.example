# Application
PORT=3000
NODE_ENV=development

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_REFRESH_SECRET=your-refresh-secret-change-in-production

# PostgreSQL
DATABASE_URL=postgres://photoshare:photoshare_secret@localhost:5432/photoshare
DATABASE_REPLICA_URL=postgres://photoshare:photoshare_secret@localhost:5433/photoshare

# Redis
REDIS_URL=redis://localhost:6379

# Neo4j
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password123

# S3 / MinIO
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin123
S3_BUCKET=photoshare-images
S3_THUMBNAIL_BUCKET=photoshare-thumbnails
S3_REGION=us-east-1

# CDN (use S3 endpoint for local dev, CloudFront/CloudFlare in production)
CDN_URL=http://localhost:9000/photoshare-images
CDN_THUMBNAIL_URL=http://localhost:9000/photoshare-thumbnails

# Elasticsearch
ELASTICSEARCH_URL=http://localhost:9200

# Rate Limiting
THROTTLE_TTL=60
THROTTLE_LIMIT=100
# Profile Screening
PROFILE_SCREEN_REVIEW_THRESHOLD=40
PROFILE_SCREEN_REJECT_THRESHOLD=70
DISPOSABLE_EMAIL_DOMAINS=mailinator.com,guerrillamail.com,10minutemail.com,temp-mail.org,yopmail.com

